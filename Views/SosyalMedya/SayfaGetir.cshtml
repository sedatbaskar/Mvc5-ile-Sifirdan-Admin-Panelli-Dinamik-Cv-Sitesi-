@model MvcCv2.Models.Entity.TblSosyalmedya
@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4> Sosyal Medya Hesap Düzenleme Sayfası</h4>
<br />
<form method="post">
    <br />
    @Html.Label("Sosyal Medya Adı")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })

    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    <br />
    @Html.Label("İcon")
    @Html.TextBoxFor(x => x.icon, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle </button>

</form>





